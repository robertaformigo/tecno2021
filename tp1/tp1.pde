 void setup(){
   size(600,600);
   background(255);
 }
 
 void draw(){
   
     strokeWeight(10);
   //lineas
   line(50,325,325,50);  // amarillo y magenta
   line(50,325,430,430); //magenta y cyan
   line(335,50,430,430); //amarillo y cyan
   line(100,120,210,500); //rojo y azul
   line(100,120,520,220); //rojo y verde
   line(210,500,520,220);
   
   
   noStroke();
   
   fill(255,255,0);
   circle(330,40,80);   //1 amarillo
   fill(251,152,19);
   circle(210,40,60);   //2 naranja
   fill(255,0,0);
   circle(100,120,80);  //3 rojo
   fill(239,18,88);
   circle(50,220,60);   //4 rosa
   fill(255,0,255);
   circle(50,325,80);   //5 magenta
   fill(100,0,100);
   circle(100,430,60);  //6 violeta
   fill(0,0,255);
   circle(210,500,80);  //7 azul
   fill(2,128,210);
   circle(330,500,60);  //8 celeste
   fill(0,255,255);
   circle(450,430,80);  //9 cyan
   fill(0,170,160);
   circle(520,325,60);  //10 azul verdoso
   fill(0,255,0);
   circle(520,220,80);  //11 verde
   fill(156,206,89);
   circle(450,110,60);  //12 verde 2
 
   

   
   
   
   
 }
